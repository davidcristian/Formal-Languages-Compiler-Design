-- Verify if a number is prime
isPrime: number = 1

n: number
input(n)

if (n <= 1) {
    isPrime = 0
} else {
    i: number = 2
    
    while (i <= n / 2 && isPrime == 1) {
        if (n % i == 0) {
            isPrime = 0
        }

        i = i + 1
    }
}

if (isPrime == 1) {
    output("Prime")
} else {
    output("Not Prime")
}
