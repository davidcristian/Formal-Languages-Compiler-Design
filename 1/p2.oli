fun p2(): void {
    -- Verify if a number is prime
    isPrime: number = 1

    n: number
    input(n)

    if (n <= 1) {
        isPrime = 0
    } else {
        for (i: number = 2; i * i <= n; i = i + 1) {
            if (n % i == 0) {
                isPrime = 0
                break
            }
        }
    }

    if (isPrime == 1) {
        output("Prime")
    } else {
        output("Not Prime")
    }
}
